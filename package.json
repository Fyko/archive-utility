{
	"name": "archive-utility",
	"version": "2.0.0",
	"description": "a Discord bot to archive text-channels",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --fix --ext .ts",
		"build": "rimraf dist/* && tsc",
		"prestart": "yarn build",
		"postbuild": "npm run copy-locales",
		"copy-locales": "cpy locales/**/*.* dist/ --parents",
		"start": "node --enable-source-maps --es-module-specifier-resolution=node .",
		"generate": "VERBOSE=1 DEBUG=1 graphql-codegen --config codegen.yml"
	},
	"imports": {
		"#util": "./dist/util/index.js",
		"#structs": "./dist/structures/index.js",
		"#const": "./dist/util/constants.js",
		"#logger": "./dist/util/logger.js",
		"#hasura": "./dist/util/hasura.js"
	},
	"author": "Carter Himmel <fyko@sycer.dev>",
	"license": "GPL-3.0",
	"dependencies": {
		"@discordjs/builders": "^0.4.0",
		"@discordjs/collection": "^0.3.2",
		"@discordjs/rest": "^0.2.0-canary.0",
		"common-tags": "^1.8.0",
		"discord.js": "^13.4.0",
		"express": "^4.17.1",
		"fs-nextra": "^0.5.1",
		"graphql": "^16.2.0",
		"graphql-request": "^3.7.0",
		"helmet": "^4.4.1",
		"i18next": "^21.6.6",
		"i18next-fs-backend": "^1.1.4",
		"mongoose": "^6.1.3",
		"node-fetch": "^2.6.1",
		"pino": "^6.13.0",
		"prom-client": "^14.0.1",
		"reflect-metadata": "^0.1.13",
		"tslib": "^2.1.0",
		"tsyringe": "^4.6.0"
	},
	"devDependencies": {
		"@graphql-codegen/add": "^3.1.1",
		"@graphql-codegen/cli": "^2.3.1",
		"@graphql-codegen/typescript": "^2.4.2",
		"@graphql-codegen/typescript-graphql-request": "^4.3.3",
		"@graphql-codegen/typescript-operations": "^2.2.2",
		"@types/common-tags": "^1.8.0",
		"@types/express": "^4.17.11",
		"@types/node": "^14.14.24",
		"@types/node-fetch": "^2.5.8",
		"@types/pino": "^6.3.11",
		"@types/ws": "^7.4.0",
		"@typescript-eslint/eslint-plugin": "^4.14.2",
		"@typescript-eslint/parser": "^4.14.2",
		"cpy-cli": "^3.1.1",
		"eslint": "^7.19.0",
		"eslint-config-marine": "^8.1.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-prettier": "^3.3.1",
		"pino-pretty": "^3.0.0-rc.2",
		"prettier": "^2.2.1",
		"rimraf": "^3.0.2",
		"typescript": "^4.1.3",
		"winston": "^3.3.3"
	},
	"optionalDependencies": {
		"bufferutil": "^4.0.3",
		"erlpack": "github:discord/erlpack",
		"utf-8-validate": "^5.0.4",
		"zlib-sync": "^0.1.7"
	},
	"volta": {
		"node": "16.13.2"
	}
}
